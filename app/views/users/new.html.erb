<div class="row">
	<div class="col-md-6">
		<h1>Register</h1>
		<%= simple_form_for @user do |f| %>
		<%= f.input :first_name %>
		<%= f.input :last_name %>
		<%= f.input :email %>
		<%= f.input :password %>
		<%= f.input :confirm_password %>
		<%= f.button :submit %>
		<% end %>
	</div>
	<div class="col-md-6">
		<%= render '/sessions/new' %>
	</div>
</div>
